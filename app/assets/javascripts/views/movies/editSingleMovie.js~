Cactus2.Views.EditMovie = Backbone.View.extend({
	template: JST['movies/editSingleMovie'],

	render: function() {
		this.$el.html(this.template());
		return this;
	},

	events: {
		'click .main'	: 'redirectToMain'
	},

	redirectToMain: function() {
		Backbone.history.navigate("new", true);
		return false;
		console.log("a");
		routerHome.navigate("main", { trigger: true });
	}
})
